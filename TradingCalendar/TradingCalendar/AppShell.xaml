<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TradingCalendar.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TradingCalendar"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Home">
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
    </FlyoutItem>
    
    <FlyoutItem Title="TradingOverview">
        <ShellContent
            Title="Trading Overview"
            ContentTemplate="{DataTemplate local:TradingOverviewPage}"
            Route="TradingOverviewPage" />
    </FlyoutItem>
    
 
    <!--<FlyoutItem Title="One" FlyoutIcon="{StaticResource IconOne}">
        <ShellContent
            Title="One"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="OnePage" />
    </FlyoutItem>-->
   

</Shell>
